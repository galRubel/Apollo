generator client {
  provider = "prisma-client-js"
}
//bycript 
datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
  url = env("DATABASE_URL")
}
  

model user {
  id        Int @id @default(autoincrement())
  usuario String
  password String
  edad     Int?
  Altura   Int?
  peso Int?
  rinonDebil Boolean?
  dialisis Boolean?
  piedrasRinon Boolean?
  actividadFisica Boolean?
  actividadCigarillo Int?
  promedioCigarillo Int?
  actividadAlchol Int?
  promedioAlchol Int?
  comidasCaseras Int?
  comidaRapida Int?
  comidaLista Int?
  comidaCongelada Int?
  creado  DateTime @default(now())

  @@map("posts")
}
